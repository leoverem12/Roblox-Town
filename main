local teamCheck = false
local fov = 150
local smoothing = 1

local RunService = game:GetService(sKwjeLsOWtrfRsqqNPWdjhjDVOeCLepSYlTNAadlZNZzaMQMVzULnEXsxWVIklDlWWjqajQpwHPHnSBdI('eiVTVVKzrCOrrWoJpcfeiruuqhmvuqqVNzeNMfwJWIeVcSLwcLgDHioqXVUcyBeTLFLeAmyXzqccVjBHjAkDHVHucDVyExGHoJRHUnVuU2VydmljZQ=='))

local FOVring = Drawing.new(sKwjeLsOWtrfRsqqNPWdjhjDVOeCLepSYlTNAadlZNZzaMQMVzULnEXsxWVIklDlWWjqajQpwHPHnSBdI('OZGOOGrUqvRIvTJXFJhaxdqyLMYuNcNdzMaCYWueRVAliiVxHcVTxiVKSIggDOfshTHAexhQjWuRYTsPZXxGFNImlExtdQZcLEdGQ2lyY2xl'))
FOVring.Visible = true
FOVring.Thickness = 1.5
FOVring.Radius = fov
FOVring.Transparency = 1
FOVring.Color = Color3.fromRGB(255, 128, 128)
FOVring.Position = workspace.CurrentCamera.ViewportSize/2

local function getClosest(cframe)
   local ray = Ray.new(cframe.Position, cframe.LookVector).Unit
   
   local target = nil
   local mag = math.huge
   
   for i,v in pairs(game.Players:GetPlayers()) do
       if v.Character and v.Character:FindFirstChild(sKwjeLsOWtrfRsqqNPWdjhjDVOeCLepSYlTNAadlZNZzaMQMVzULnEXsxWVIklDlWWjqajQpwHPHnSBdI('IJJpyNEmoYNKePexHwBsLtestJqTlWRyqmxoDFuuOCqCNqDGOYuKMeNOlLMHxcXTuisUuGRVJgjiSZIXrKswgxaCfIXpijHodcjHSGVhZA==')) and v.Character:FindFirstChild(sKwjeLsOWtrfRsqqNPWdjhjDVOeCLepSYlTNAadlZNZzaMQMVzULnEXsxWVIklDlWWjqajQpwHPHnSBdI('hDmdYcKpNehjOzwfMsbuHAuNtKMXAKQdYSTyOCmRtncHhYvnJtLnIITEJcMehpKDfiIDHbZFTecRQblmPhbexgmYNrQOZaKlkIrtSHVtYW5vaWQ=')) and v.Character:FindFirstChild(sKwjeLsOWtrfRsqqNPWdjhjDVOeCLepSYlTNAadlZNZzaMQMVzULnEXsxWVIklDlWWjqajQpwHPHnSBdI('cgIgrRyiswJOKQSCSCdoVmGZWkMWWfEcvSUOdHNskziVBKxSuHEMVUeYVcOuMKWmRpqHoYRqDPqSNJxQprXzvVuwgXRKymSVdWrwSHVtYW5vaWRSb290UGFydA==')) and v ~= game.Players.LocalPlayer and (v.Team ~= game.Players.LocalPlayer.Team or (not teamCheck)) then
           local magBuf = (v.Character.Head.Position - ray:ClosestPoint(v.Character.Head.Position)).Magnitude
           
           if magBuf < mag then
               mag = magBuf
               target = v
           end
       end
   end
   
   return target
end

loop = RunService.RenderStepped:Connect(function()
   local UserInputService = game:GetService(sKwjeLsOWtrfRsqqNPWdjhjDVOeCLepSYlTNAadlZNZzaMQMVzULnEXsxWVIklDlWWjqajQpwHPHnSBdI('QohbpdVvDZijcSYNXKcNxLHJWWXCoqCSOMtCHjWjYhDOCiUnMSRTWWCEbMRjmtgARzjStULtVBSwSlMOjyPuezcZqpGCIUutrolAVXNlcklucHV0U2VydmljZQ=='))
   local pressed = --[[UserInputService:IsKeyDown(Enum.KeyCode.E)]] UserInputService:IsMouseButtonPressed(Enum.UserInputType.MouseButton2) --Enum.UserInputType.MouseButton2
   local localPlay = game.Players.localPlayer.Character
   local cam = workspace.CurrentCamera
   local zz = workspace.CurrentCamera.ViewportSize/2
   
   if pressed then
       local Line = Drawing.new(sKwjeLsOWtrfRsqqNPWdjhjDVOeCLepSYlTNAadlZNZzaMQMVzULnEXsxWVIklDlWWjqajQpwHPHnSBdI('VOhDFItetaTxnxjvdmPmwopVNkzeIINPoywNKYQftvMENJDlaNJiqboJVZRRKBHVuFlIGYtHxbxoPXtSEjAKTntrZpFCxtpFeeOCTGluZQ=='))
       local curTar = getClosest(cam.CFrame)
       local ssHeadPoint = cam:WorldToScreenPoint(curTar.Character.Head.Position)
       ssHeadPoint = Vector2.new(ssHeadPoint.X, ssHeadPoint.Y)
       if (ssHeadPoint - zz).Magnitude < fov then
           workspace.CurrentCamera.CFrame = workspace.CurrentCamera.CFrame:Lerp(CFrame.new(cam.CFrame.Position, curTar.Character.Head.Position), smoothing)
       end
   end
   
   if UserInputService:IsKeyDown(Enum.KeyCode.Delete) then
       loop:Disconnect()
       FOVring:Remove()
   end
end)    
